<h1>Laser Cutter Control Panel</h1>

<div id = "upload_button">
    <h1>Select File</h1>
    <button class="file-upload">
        <input type="file" class="file-input">Choose File
    </button>
</div>
<br>
<div class="control-panel-xy">
        <h1>X/Y</h1>
        <div class="up_button_xy">
            <button class="btn box" id="control-yinc"><i class="icon-arrow-up"></i></button>
        </div>
        <div>
            <button class="btn box" id="control-xdec"><i class="icon-arrow-left"></i></button>
            <button class="btn box" id="control-xyhome"><i class="icon-home"></i></button>
            <button class="btn box" id="control-xinc"><i class="icon-arrow-right"></i></button>
        </div>
        <div class="down_button_xy">
            <button class="btn box" id="control-ydec"><i class="icon-arrow-down"></i></button>
        </div>
</div>
<div class="control-panel-z">
        <h1>Z</h1>
        <div class="up_button_z">
            <button class="btn box" id="control-zyinc"><i class="icon-arrow-up"></i></button>
        </div>
        <div class = "home_button_z">
            <button class="btn box" id="control-zhome"><i class="icon-home"></i></button>
        </div>
        <div class="down_button_z">
            <button class="btn box" id="control-zydec"><i class="icon-arrow-down"></i></button>
        </div>
</div>
